{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            2, {
                "args": "after-used",
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ]
    },
    "env": {
        "es6": true,
        "node": true,
        "amd": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint"
}